{
  "manifest_version": 2,
  "name": "Amazon Whole Foods Delivery Slots Finder",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.com/gp/buy/shipoptionselect/handlers/*"
      ],
      "js": ["delivery_slots.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
